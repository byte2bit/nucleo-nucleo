                    "url": "https://code.highcharts.com/10.0.0/highcharts-3d.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "^1.0 || ^2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fullcalendar/fullcalendar",
                "version": "3.9.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/fullcalendar/fullcalendar/archive/v3.9.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "google/charts",
